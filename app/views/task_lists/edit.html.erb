<h1><%= @project.title %></h1>

<h3>Edit Task List</h3>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@task_list, url: project_task_list_path(@project, @task_list)) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Update", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
