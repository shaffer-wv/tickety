<h1><%= @project.title %></h1>

<h3>Add Task List</h3>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@task_list, url: project_task_lists_path) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Add task list", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
